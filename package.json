{
    "name": "@actdim/dynstruct",
    "version": "0.9.0",
    "description": "A type-safe component system for large-scale apps: explicit dependencies, message bus communication, and structure-first, declarative design",
    "author": "Pavel Borodaev",
    "license": "Proprietary",
    "type": "module",
    "repository": {
        "type": "git",
        "url": "https://github.com/actdim/dynstruct.git"
    },
    "bugs": {
        "url": "https://github.com/actdim/dynstruct/issues"
    },
    "homepage": "https://github.com/actdim/dynstruct/#readme",
    "keywords": [
        "typescript",
        "components",
        "react",
        "component-model",
        "architecure",
        "modularity",
        "structure",
        "communication"
    ],
    "files": [
        "dist",
        "docs",
        "README.md",
        "LICENSE"
    ],
    "files(src)": [
        "src",
        "docs",
        "README.md",
        "LICENSE"
    ],
    "exports": {
        "./*": {
            "import": "./dist/*.es.js",
            "types": "./dist/*.d.ts"
        }
    },
    "typesVersions": {
        "*": {
            "*": [
                "dist/*.d.ts"
            ]
        }
    },
    "sideEffects": false,
    "sideEffects?": [],
    "scripts": {
        "build": "tsc && vite build",
        "test": "npx vitest",
        "wtest": "npx vitest --watch",
        "dtest": "node --inspect-brk node_modules/vitest/vitest.mjs run --poolOptions.threads.singleThread",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0"
    },
    "peerDependencies": {
        "rxjs": "^7.8.1",
        "uuid": "^9.0.1",
        "@actdim/utico": "^0.9.0",
        "@actdim/msgmesh": "^0.9.1",
        "http-status": "^1.7.3",
        "jwt-decode": "^4.0.0",
        "mobx": "^6.13.7",
        "mobx-react-lite": "^4.1.0",
        "mobx-utils": "^6.1.1",
        "path-to-regexp": "^8.2.0",
        "react": "^19.1.0",
        "react-dom": "^19.1.0",
        "react-router": "^7.6.2",
        "react-router-dom": "^7.6.2"
    },
    "devDependencies": {
        "@types/node": "^20.11.30",
        "@types/uuid": "^9.0.7",
        "@typescript-eslint/eslint-plugin": "^6.10.0",
        "@typescript-eslint/parser": "^6.10.0",
        "@vitejs/plugin-react-swc": "^3.5.0",
        "eslint": "^8.53.0",
        "eslint-import-resolver-typescript": "^4.3.4",
        "path": "^0.12.7",
        "tslib": "^2.8.1",
        "typescript": "^5.8.3",
        "vite": "^5.4.18",
        "vite-plugin-dts": "^4.5.3",
        "vite-tsconfig-paths": "^5.1.4",
        "vitest": "^3.1.2"
    }
}
